{
  "name": "vue-html-bridge-core",
  "version": "0.0.1",
  "description": "Static analysis bridge converting Vue.js SFCs into validatable HTML snapshots",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "author": "nagashimam <masato.ngsm@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "html",
    "static-analysis",
    "sfc",
    "validation"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/parser": "^7.28.6",
    "@prettier/sync": "^0.5.2",
    "@vue/compiler-dom": "^3.5.27",
    "@vue/compiler-sfc": "^3.5.27",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@babel/types": "^7.28.6"
  }
}
